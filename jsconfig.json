{
  "include": [
    "./src/**/*"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@@/*":["./src/store/modules/*"]
    }
  },
  "exclude": ["node_modules", "dist"],
}